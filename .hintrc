{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-text-size-adjust",
          "@-ms-viewport",
          "@-ms-viewport"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 110",
    "not firefox <= 112",
    "not and_chr <= 112",
    "not chrome <= 112",
    "not edge <= 112",
    "not ios_saf <= 16.4",
    "not opera <= 95",
    "not safari <= 16.4",
    "not samsung <= 20"
  ]
}